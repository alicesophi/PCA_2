//Faça um Programa que pergunte quanto você ganha por hora e o número de horas trabalhadas no mês. Calcule e mostre o total do seu salário no referido mês.
#include <stdio.h>

int main(void){
    int horas_trabalhadas_mes;;
	float salario_hora, salario_total;

	printf("Digite o seu salario/hora: ");
	scanf("%f", &salario_hora);
	
	printf("Digite suas horas trabalhadas no mes: ");
	scanf("%d", &horas_trabalhadas_mes);

	salario_total = salario_hora * horas_trabalhadas_mes;
    
 	printf("O seu salario neste mes eh: %.2f", salario_total);

	return 0;

}
